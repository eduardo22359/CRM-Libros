@{ ViewData["Title"] = "Clientes > Detalle"; }

@model Cliente

<dl>
    <dt>Id</dt>
    <dd>@Model.Id</dd>

    <dt>Nombre</dt>
    <dd>@Model.Nombre</dd>

    <dt>Direccion</dt>
    <dd>@Model.Direccion</dd>

    <dt>Telefono</dt>
    <dd>@Model.Telefono</dd>

    <dt>Correo</dt>
    <dd>@Model.Correo</dd>
</dl>
